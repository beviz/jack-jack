{
  "name": "Jack-Jack",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "webpack": "cd webpack && npm run webpack",
    "build-darwin": "electron-packager . jack-jack --ignore=webpack --overwrite --app-version=0.1.1 --icon=assets/logo.icns --platform=darwin --arch=x64 --electron-version=2.0.4",
    "build-win32": "electron-packager . jack-jack --ignore=webpack --overwrite --app-version=0.1.1 --icon=assets/logo.ico --platform=win32 --arch=x64 --electron-version=2.0.4",
    "watch": "cd webpack && npm run watch"
  },
  "repository": "https://github.com/beviz/jack-jack",
  "keywords": [
    "Electron",
    "command",
    "shell"
  ],
  "author": "Bevis",
  "license": "Apache-2.0",
  "dependencies": {
    "electron-json-storage": "^4.1.0"
  }
}
